{
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 150,
    "tabWidth": 4,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "proseWrap": "preserve",
    "embeddedLanguageFormatting": "auto",
    "plugins": ["prettier-plugin-tailwindcss"]
}
